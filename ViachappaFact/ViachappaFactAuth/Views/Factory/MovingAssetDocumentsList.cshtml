@model IEnumerable<ViachappaFactAuth.DB.MovingAssetDocument>

@{
    ViewBag.Title = "MovingAssetDocumentList";
}

<h2>MovingAssetDocumentsList</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DocDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OperDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DocNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DocDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OperDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DocNumber)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
